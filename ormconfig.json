{
  "type": "${DB_TYPE}",
  "host": "${DB_HOST}",
  "port": "${DB_PORT}",
  "username": "${DB_USERNAME}",
  "password": "${DB_PASSWORD}",
  "database": "${DB_DATABASE}",
  "entities": ["dist/**/*.entity{.ts,.js}"],
  "migrations": ["dist/migrations/*{.ts,.js}"],
  "migrationsRun": false,
  "synchronize": false,
  "logging": "${DB_LOGGING}",
  "cli": {   
    "migrationsDir": "src/migrations"
  }
}